{
    "version": 2,
    "name": "journal-cms",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/api/$1" }
    ],
    "redirects": [
      { "source": "/api", "destination": "/", "statusCode": 301 }
    ]
 }